<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="childTaskFilledVM" type="com.syedzulqarnainsapplication.app.modules.childtaskfilled.data.viewmodel.ChildTaskFilledVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearColumnarrowleft"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylegray_200">
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearAppbartop"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStyleteal_400">
            <androidx.appcompat.widget.Toolbar
                android:gravity="center_horizontal"
                android:id="@+id/toolbarToolbar"
                android:layout_height="wrap_content"
                android:layout_marginHorizontal="@dimen/_19pxh"
                android:layout_marginVertical="@dimen/_19pxv"
                android:layout_width="match_parent" app:contentInsetStart="0dp">
                <LinearLayout android:id="@+id/linearRowarrowleft"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageArrowleft"
                        android:layout_height="@dimen/_18pxh"
                        android:layout_width="@dimen/_18pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrowleft"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtVaccinatorTask"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_56pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_weight="0.83"
                        android:layout_width="0dp"
                        android:lineSpacingExtra="@dimen/_1pxh"
                        android:singleLine="true"
                        android:text="@{childTaskFilledVM.childTaskFilledModel.txtVaccinatorTask}"
                        style="@style/txtIntermedium16" tools:text="@string/msg_child_vaccinati"/>
                    <ImageView android:id="@+id/imageHome"
                        android:layout_height="@dimen/_18pxh"
                        android:layout_marginStart="@dimen/_56pxh"
                        android:layout_width="@dimen/_18pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_home"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_home"/>
                </LinearLayout>
            </androidx.appcompat.widget.Toolbar>
        </LinearLayout>
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearChildtaskfi"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_16pxh"
            android:layout_marginStart="@dimen/_16pxh"
            android:layout_marginTop="@dimen/_17pxv"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylegray_200">
            <FrameLayout android:id="@+id/frameItems"
                android:layout_gravity="start"
                android:layout_height="@dimen/_759pxv"
                android:layout_width="@dimen/_328pxh" style="@style/groupStylegray_200">
                <View android:background="@color/gray_200"
                    android:id="@+id/viewCreatefromfra"
                    android:layout_gravity="top|start"
                    android:layout_height="@dimen/_703pxh"
                    android:layout_marginBottom="@dimen/_10pxv" android:layout_width="@dimen/_328pxh"/>
                <LinearLayout android:id="@+id/linearItems"
                    android:layout_gravity="start"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="vertical">
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearRowsettings"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:orientation="horizontal" style="@style/groupStylewhite_A700cornerRadius">
                        <ImageButton
                            android:background="@drawable/rectangle_bg_amber_200_radius_20"
                            android:id="@+id/btnSettings"
                            android:layout_height="@dimen/_40pxh"
                            android:layout_marginBottom="@dimen/_42pxv"
                            android:layout_marginStart="@dimen/_16pxh"
                            android:layout_marginTop="@dimen/_16pxv"
                            android:layout_width="@dimen/_40pxh"
                            android:paddingHorizontal="@dimen/_7pxh"
                            android:paddingVertical="@dimen/_7pxv"
                            android:scaleType="fitCenter"
                            android:src="@drawable/img_settings" tools:ignore="ContentDescription"/>
                        <LinearLayout android:gravity="start"
                            android:id="@+id/linearColumntextaddressone"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_18pxv"
                            android:layout_marginEnd="@dimen/_141pxh"
                            android:layout_marginStart="@dimen/_16pxh"
                            android:layout_marginTop="@dimen/_18pxv"
                            android:layout_weight="0.74"
                            android:layout_width="0dp" android:orientation="vertical">
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtTextaddressOne"
                                android:layout_height="wrap_content"
                                android:layout_marginEnd="@dimen/_10pxh"
                                android:layout_width="match_parent"
                                android:singleLine="true"
                                android:text="@{childTaskFilledVM.childTaskFilledModel.txtTextaddressOne}"
                                style="@style/txtIntersemibold16" tools:text="@string/lbl_shahzain_ali"/>
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtVridCounter"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/_8pxv"
                                android:layout_width="match_parent"
                                android:singleLine="true"
                                android:text="@{childTaskFilledVM.childTaskFilledModel.txtVridCounter}"
                                style="@style/txtIntermedium14_1" tools:text="@string/lbl_vr_id_89389304"/>
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtVRIDAG202519"
                                android:layout_height="wrap_content"
                                android:layout_marginEnd="@dimen/_10pxh"
                                android:layout_marginTop="@dimen/_8pxv"
                                android:layout_width="match_parent"
                                android:singleLine="true"
                                android:text="@{childTaskFilledVM.childTaskFilledModel.txtVRIDAG202519}"
                                style="@style/txtIntermedium14_2" tools:text="@string/lbl_zero_dose"/>
                        </LinearLayout>
                    </LinearLayout>
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearDatacard"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_16pxv"
                        android:layout_width="match_parent"
                        android:orientation="vertical" style="@style/groupStylewhite_A700cornerRadius">
                        <LinearLayout
                            android:gravity="center_horizontal"
                            android:id="@+id/linearHeading"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:orientation="vertical" style="@style/groupStylegray_100">
                            <TextView android:ellipsize="end"
                                android:gravity="start"
                                android:id="@+id/txtText"
                                android:layout_height="match_parent"
                                android:layout_marginBottom="@dimen/_19pxv"
                                android:layout_marginEnd="@dimen/_16pxh"
                                android:layout_marginStart="@dimen/_16pxh"
                                android:layout_marginTop="@dimen/_18pxv"
                                android:layout_width="match_parent"
                                android:singleLine="true"
                                android:text="@{childTaskFilledVM.childTaskFilledModel.txtText}"
                                style="@style/txtIntersemibold14" tools:text="@string/lbl_child_details"/>
                        </LinearLayout>
                        <androidx.recyclerview.widget.RecyclerView
                            android:id="@+id/recyclerListage"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:orientation="vertical"
                            app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                            itemSpace="@{@dimen/_1pxh}"
                            tools:itemCount="2" tools:listitem="@layout/row_listage3"/>
                    </LinearLayout>
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearDatacard1"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_16pxv"
                        android:layout_width="match_parent"
                        android:orientation="vertical" style="@style/groupStylewhite_A700cornerRadius">
                        <LinearLayout
                            android:gravity="center_horizontal"
                            android:id="@+id/linearHeading1"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:orientation="vertical" style="@style/groupStylegray_100">
                            <TextView android:ellipsize="end"
                                android:gravity="start"
                                android:id="@+id/txtTextOne"
                                android:layout_height="match_parent"
                                android:layout_marginBottom="@dimen/_16pxv"
                                android:layout_marginEnd="@dimen/_16pxh"
                                android:layout_marginStart="@dimen/_16pxh"
                                android:layout_marginTop="@dimen/_20pxv"
                                android:layout_width="match_parent"
                                android:singleLine="true"
                                android:text="@{childTaskFilledVM.childTaskFilledModel.txtTextOne}"
                                style="@style/txtIntersemibold14" tools:text="@string/msg_demographic_det"/>
                        </LinearLayout>
                        <androidx.recyclerview.widget.RecyclerView
                            android:id="@+id/recyclerListmothername"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:orientation="vertical"
                            app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                            itemSpace="@{@dimen/_1pxh}"
                            tools:itemCount="3" tools:listitem="@layout/row_listmothername3"/>
                    </LinearLayout>
                    <LinearLayout android:gravity="center_horizontal"
                        android:id="@+id/linearActioncard"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_16pxv"
                        android:layout_width="match_parent"
                        android:orientation="vertical" style="@style/groupStylewhite_A700cornerRadius">
                        <EditText
                            android:background="@drawable/rectangle_bg_gray_100"
                            android:ellipsize="end"
                            android:hint="@string/lbl_actions"
                            android:id="@+id/etHeading"
                            android:inputType="text"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:paddingBottom="@dimen/_19pxv"
                            android:paddingEnd="@dimen/_30pxh"
                            android:paddingStart="@dimen/_16pxh"
                            android:paddingTop="@dimen/_18pxv"
                            android:singleLine="true"
                            android:text="@={childTaskFilledVM.childTaskFilledModel.etHeadingValue}"
                            android:textColorHint="@color/bluegray_900"
                            style="@style/etSolid" tools:ignore="Autofill"/>
                        <Spinner
                            android:background="@drawable/layer_list_bg_white_a700_border_gray_400_radius_4"
                            android:gravity="center_horizontal"
                            android:id="@+id/spinnerField"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_16pxh"
                            android:layout_marginStart="@dimen/_16pxh"
                            android:layout_marginTop="@dimen/_16pxv"
                            android:layout_width="match_parent" tools:listitem="@layout/spinner_item"/>
                        <androidx.appcompat.widget.AppCompatButton
                            android:ellipsize="end"
                            android:gravity="center"
                            android:id="@+id/btnCompleteActionOne"
                            android:layout_gravity="end"
                            android:layout_height="@dimen/_40pxv"
                            android:layout_marginHorizontal="@dimen/_16pxh"
                            android:layout_marginVertical="@dimen/_16pxv"
                            android:layout_width="@dimen/_183pxh"
                            android:paddingBottom="@dimen/_13pxv"
                            android:paddingEnd="@dimen/_24pxh"
                            android:paddingStart="@dimen/_24pxh"
                            android:paddingTop="@dimen/_13pxv"
                            android:singleLine="true"
                            android:text="@string/lbl_complete_action"
                            android:textAllCaps="false"
                            style="@style/btnSolidRounded" tools:text="@string/lbl_complete_action"/>
                    </LinearLayout>
                    <View android:background="@color/gray_200"
                        android:id="@+id/viewMarginbottom"
                        android:layout_height="@dimen/_40pxv"
                        android:layout_marginTop="@dimen/_16pxv" android:layout_width="@dimen/_328pxh"/>
                </LinearLayout>
            </FrameLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
