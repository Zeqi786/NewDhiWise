<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="capturedImageFourVM" type="com.syedzulqarnainsapplication.app.modules.capturedimagefour.data.viewmodel.CapturedImageFourVM"/>
    </data>
    <FrameLayout android:id="@+id/frameCapturedimage"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylegray_200">
        <FrameLayout android:id="@+id/frameStackvaccinationsho"
            android:layout_gravity="start"
            android:layout_height="match_parent" android:layout_width="match_parent">
            <ImageView android:id="@+id/imageVaccinationsho"
                android:layout_gravity="start"
                android:layout_height="match_parent"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                imageRes="@{@drawable/img_vaccinationsho_776x360}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_vaccinationsho_776x360"/>
            <LinearLayout android:id="@+id/linearColumnclose"
                android:layout_gravity="center"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_24pxv"
                android:layout_marginEnd="@dimen/_16pxh"
                android:layout_marginStart="@dimen/_11pxh"
                android:layout_marginTop="@dimen/_19pxv"
                android:layout_width="match_parent" android:orientation="vertical">
                <ImageView android:id="@+id/imageClose"
                    android:layout_gravity="start"
                    android:layout_height="@dimen/_33pxh"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_width="@dimen/_33pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_close"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_close"/>
                <LinearLayout android:gravity="end"
                    android:id="@+id/linearRowarrowright"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_659pxv"
                    android:layout_width="wrap_content"
                    android:orientation="horizontal" style="@style/groupStyleteal_400cornerRadius">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtButtontext"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_13pxv"
                        android:layout_marginStart="@dimen/_24pxh"
                        android:layout_marginTop="@dimen/_12pxv"
                        android:layout_weight="0.82"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{capturedImageFourVM.capturedImageFourModel.txtButtontext}"
                        style="@style/txtIntersemibold14_1" tools:text="@string/lbl_submit"/>
                    <ImageView android:id="@+id/imageArrowright"
                        android:layout_height="@dimen/_12pxh"
                        android:layout_marginBottom="@dimen/_14pxv"
                        android:layout_marginEnd="@dimen/_26pxh"
                        android:layout_marginStart="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_14pxv"
                        android:layout_width="@dimen/_12pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrowright"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_arrowright"/>
                </LinearLayout>
            </LinearLayout>
        </FrameLayout>
        <LinearLayout android:id="@+id/linearColumnOne"
            android:layout_gravity="start"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylebluegray_900_7f">
            <LinearLayout android:gravity="start"
                android:id="@+id/linearColumn"
                android:layout_height="match_parent"
                android:layout_marginBottom="@dimen/_360pxv"
                android:layout_marginEnd="@dimen/_24pxh"
                android:layout_marginStart="@dimen/_24pxh"
                android:layout_marginTop="@dimen/_240pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylewhite_A700cornerRadius">
                <androidx.appcompat.widget.AppCompatButton
                    android:ellipsize="end" android:gravity="center"
                    android:id="@+id/btnImageStored"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_18pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_30pxh"
                    android:paddingTop="@dimen/_18pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_image_stored"
                    android:textAllCaps="false"
                    style="@style/btnSolidRounded_1" tools:text="@string/lbl_image_stored"/>
                <LinearLayout android:gravity="center_horizontal"
                    android:id="@+id/linearColumntext"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:orientation="vertical" style="@style/groupStylewhite_9">
                    <TextView android:gravity="center_horizontal"
                        android:id="@+id/txtText"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_35pxh"
                        android:layout_marginStart="@dimen/_35pxh"
                        android:layout_marginTop="@dimen/_20pxv"
                        android:layout_width="match_parent"
                        android:text="@{capturedImageFourVM.capturedImageFourModel.txtText}"
                        style="@style/txtInterregular14" tools:text="@string/msg_the_image_for_v"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtTextleft"
                        android:layout_gravity="center_horizontal"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_27pxv"
                        android:layout_marginEnd="@dimen/_35pxh"
                        android:layout_marginStart="@dimen/_35pxh"
                        android:layout_marginTop="@dimen/_26pxv"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{capturedImageFourVM.capturedImageFourModel.txtTextleft}"
                        android:textAllCaps="true"
                        style="@style/txtIntersemibold14_2" tools:text="@string/lbl_okay"/>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </FrameLayout>
</layout>
