<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="editVaccinationRecordStep2SelectedTwentythreeVM" type="com.syedzulqarnainsapplication.app.modules.editvaccinationrecordstep2selectedtwentythree.data.viewmodel.EditVaccinationRecordStep2SelectedTwentythreeVM"/>
    </data>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearEditvaccinatio"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylegray_200">
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearAppbartop"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStyleteal_400">
            <androidx.appcompat.widget.Toolbar android:gravity="start"
                android:id="@+id/toolbarToolbar"
                android:layout_height="wrap_content"
                android:layout_marginHorizontal="@dimen/_19pxh"
                android:layout_marginVertical="@dimen/_19pxv"
                android:layout_width="match_parent" app:contentInsetStart="0dp">
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearRowarrowleft"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageArrowleft"
                        android:layout_height="@dimen/_18pxh"
                        android:layout_width="@dimen/_18pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrowleft"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtVaccinatorTask"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_64pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_weight="0.91"
                        android:layout_width="0dp"
                        android:lineSpacingExtra="@dimen/_1pxh"
                        android:singleLine="true"
                        android:text="@{editVaccinationRecordStep2SelectedTwentythreeVM.editVaccinationRecordStep2SelectedTwentythreeModel.txtVaccinatorTask}"
                        style="@style/txtIntermedium16" tools:text="@string/msg_edit_vaccinatio2"/>
                </LinearLayout>
            </androidx.appcompat.widget.Toolbar>
        </LinearLayout>
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearItems"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylegray_200">
            <LinearLayout android:gravity="start"
                android:id="@+id/linearDatacard"
                android:layout_height="match_parent"
                android:layout_marginBottom="@dimen/_311pxv"
                android:layout_marginEnd="@dimen/_16pxh"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_17pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylecornerRadius">
                <LinearLayout android:id="@+id/linearRowtext"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:orientation="horizontal" style="@style/groupStylegray_100">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtText"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_19pxv"
                        android:layout_marginStart="@dimen/_16pxh"
                        android:layout_marginTop="@dimen/_18pxv"
                        android:layout_weight="0.64"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{editVaccinationRecordStep2SelectedTwentythreeVM.editVaccinationRecordStep2SelectedTwentythreeModel.txtText}"
                        style="@style/txtIntersemibold14" tools:text="@string/msg_vaccination_det"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtTextOne"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_16pxv"
                        android:layout_marginEnd="@dimen/_16pxh"
                        android:layout_marginStart="@dimen/_92pxh"
                        android:layout_marginTop="@dimen/_20pxv"
                        android:layout_weight="0.36"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{editVaccinationRecordStep2SelectedTwentythreeVM.editVaccinationRecordStep2SelectedTwentythreeModel.txtTextOne}"
                        style="@style/txtIntersemibold14_2" tools:text="@string/lbl_step_2_of_2"/>
                </LinearLayout>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearColumnblockinformati"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:orientation="vertical" style="@style/groupStylewhite_A700cornerRadius">

                    <com.google.android.material.textfield.TextInputLayout
                        android:hint="@string/msg_vaccination_sta"
                        android:id="@+id/textInputLayBlockinformati"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_16pxh"
                        android:layout_marginStart="@dimen/_16pxh"
                        android:layout_marginTop="@dimen/_16pxv"
                        android:layout_width="match_parent"
                        android:paddingBottom="@dimen/_13pxv"
                        android:paddingEnd="@dimen/_30pxh"
                        android:paddingStart="@dimen/_16pxh"
                        android:paddingTop="@dimen/_12pxv"
                        android:textColorHint="@color/bluegray_500" style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox">

                        <com.google.android.material.textfield.TextInputEditText
                            android:ellipsize="end"
                            android:fontFamily="@font/inter"
                            android:id="@+id/etBlockinformati"
                            android:inputType="text"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@={editVaccinationRecordStep2SelectedTwentythreeVM.editVaccinationRecordStep2SelectedTwentythreeModel.etBlockinformatiValue}"
                            android:textColor="@color/gray_500"
                            android:textFontWeight="400"
                            android:textSize="@dimen/_14pxh"
                            android:textStyle="normal" tools:text="@string/lbl_unvaccinated"/>
                    </com.google.android.material.textfield.TextInputLayout>

                    <com.google.android.material.textfield.TextInputLayout
                        android:hint="@string/lbl_reason"
                        android:id="@+id/textInputLayTextTwo"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_16pxh"
                        android:layout_marginStart="@dimen/_16pxh"
                        android:layout_marginTop="@dimen/_16pxv"
                        android:layout_width="match_parent"
                        android:paddingBottom="@dimen/_13pxv"
                        android:paddingEnd="@dimen/_30pxh"
                        android:paddingStart="@dimen/_16pxh"
                        android:paddingTop="@dimen/_12pxv"
                        android:textColorHint="@color/bluegray_500" style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox">

                        <com.google.android.material.textfield.TextInputEditText
                            android:ellipsize="end"
                            android:fontFamily="@font/inter"
                            android:id="@+id/etTextTwo"
                            android:inputType="text"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@={editVaccinationRecordStep2SelectedTwentythreeVM.editVaccinationRecordStep2SelectedTwentythreeModel.etTextTwoValue}"
                            android:textColor="@color/bluegray_900"
                            android:textFontWeight="400"
                            android:textSize="@dimen/_14pxh"
                            android:textStyle="normal" tools:text="@string/msg_fear_of_fever_a3"/>
                    </com.google.android.material.textfield.TextInputLayout>
                    <View android:background="@color/gray_300"
                        android:id="@+id/lineLineTwo"
                        android:layout_height="@dimen/_1pxv"
                        android:layout_marginTop="@dimen/_16pxv" android:layout_width="@dimen/_328pxh"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtLabel"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_16pxh"
                        android:layout_marginStart="@dimen/_16pxh"
                        android:layout_marginTop="@dimen/_19pxv"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{editVaccinationRecordStep2SelectedTwentythreeVM.editVaccinationRecordStep2SelectedTwentythreeModel.txtLabel}"
                        style="@style/txtInterregular14_1" tools:text="@string/msg_is_the_family_w"/>
                    <Spinner
                        android:background="@drawable/layer_list_bg_white_a700_border_gray_400_radius_4"
                        android:gravity="center_horizontal"
                        android:id="@+id/spinnerField"
                        android:layout_height="wrap_content"
                        android:layout_marginHorizontal="@dimen/_16pxh"
                        android:layout_marginVertical="@dimen/_16pxv"
                        android:layout_width="match_parent" tools:listitem="@layout/spinner_item"/>
                </LinearLayout>
                <LinearLayout android:id="@+id/linearRowsubmit"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_24pxv"
                    android:layout_marginTop="@dimen/_24pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <LinearLayout
                        android:id="@+id/linearRowarrowleftOne"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_12pxv"
                        android:layout_marginStart="@dimen/_1pxh"
                        android:layout_marginTop="@dimen/_12pxv"
                        android:layout_weight="0.41"
                        android:layout_width="0dp" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageArrowleftOne"
                            android:layout_height="@dimen/_14pxh"
                            android:layout_marginTop="@dimen/_1pxv"
                            android:layout_width="@dimen/_14pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_arrowleft_14x14"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft_14x14"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtTextbtn"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_marginStart="@dimen/_17pxh"
                            android:layout_weight="0.73"
                            android:layout_width="0dp"
                            android:singleLine="true"
                            android:text="@{editVaccinationRecordStep2SelectedTwentythreeVM.editVaccinationRecordStep2SelectedTwentythreeModel.txtTextbtn}"
                            android:textAllCaps="true"
                            style="@style/txtIntersemibold14_3" tools:text="@string/lbl_back"/>
                    </LinearLayout>
                    <androidx.appcompat.widget.AppCompatButton
                        android:ellipsize="end" android:gravity="center"
                        android:id="@+id/btnSubmit"
                        android:layout_height="@dimen/_40pxv"
                        android:layout_marginStart="@dimen/_154pxh"
                        android:layout_width="@dimen/_103pxh"
                        android:paddingBottom="@dimen/_13pxv"
                        android:paddingEnd="@dimen/_24pxh"
                        android:paddingStart="@dimen/_24pxh"
                        android:paddingTop="@dimen/_13pxv"
                        android:singleLine="true"
                        android:text="@string/lbl_submit"
                        android:textAllCaps="false"
                        style="@style/btnSolidRounded" tools:text="@string/lbl_submit"/>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
