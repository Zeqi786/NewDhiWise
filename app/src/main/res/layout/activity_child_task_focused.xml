                            <TextView android:ellipsize="end"
                                android:id="@+id/txtTextbartoolcOne"
                                android:layout_height="wrap_content"
                                android:layout_marginEnd="@dimen/_16pxh"
                                android:layout_marginStart="@dimen/_16pxh"
                                android:layout_marginTop="@dimen/_19pxv"
                                android:layout_width="wrap_content"
                                android:singleLine="true"
                                android:text="@{childTaskFocusedVM.childTaskFocusedModel.txtTextbartoolcOne}"
                                style="@style/txtInterregular14_1" tools:text="@string/lbl_deceased"/>
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtTextbartoolcTwo"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/_24pxv"
                                android:layout_marginEnd="@dimen/_16pxh"
                                android:layout_marginStart="@dimen/_16pxh"
                                android:layout_marginTop="@dimen/_19pxv"
                                android:layout_width="wrap_content"
