<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="drawerItemMenuModel" type="com.syedzulqarnainsapplication.app.modules.itemmenu.data.model.DrawerItemMenuModel"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearMenu"
        android:layout_height="match_parent"
        android:layout_width="match_parent" android:orientation="vertical">
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearColumn"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylebluegray_900_7f">
            <LinearLayout android:gravity="start"
                android:id="@+id/linearColumnhome"
                android:layout_height="match_parent"
                android:layout_marginEnd="@dimen/_102pxh"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylewhite_A700">
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearItems"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_26pxh"
                    android:layout_marginStart="@dimen/_26pxh"
                    android:layout_marginTop="@dimen/_82pxv"
                    android:layout_width="wrap_content" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageHome"
                        android:layout_height="@dimen/_15pxh"
                        android:layout_width="@dimen/_15pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_home_15x15"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_home_15x15"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtHomeOne"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_18pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_weight="0.73"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{drawerItemMenuModel.txtHomeOne}"
                        style="@style/txtInterregular14_1" tools:text="@string/lbl_home"/>
                </LinearLayout>
                <TextView android:ellipsize="end"
                    android:gravity="center_horizontal"
                    android:id="@+id/txtHomeTwo"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_24pxh"
                    android:layout_marginStart="@dimen/_24pxh"
                    android:layout_marginTop="@dimen/_39pxv"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{drawerItemMenuModel.txtHomeTwo}"
                    style="@style/txtInterregular14_1" tools:text="@string/msg_follow_up_task"/>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearItems1"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_25pxh"
                    android:layout_marginStart="@dimen/_25pxh"
                    android:layout_marginTop="@dimen/_33pxv"
                    android:layout_width="wrap_content" android:orientation="horizontal">
                    <ImageView android:id="@+id/imagePlus"
                        android:layout_height="@dimen/_17pxh"
                        android:layout_width="@dimen/_17pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_plus_17x17"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_plus_17x17"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtHomeThree"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_17pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_weight="0.84"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{drawerItemMenuModel.txtHomeThree}"
                        style="@style/txtInterregular14_1" tools:text="@string/lbl_add_structure"/>
                </LinearLayout>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearItems2"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_24pxh"
                    android:layout_marginStart="@dimen/_24pxh"
                    android:layout_marginTop="@dimen/_34pxv"
                    android:layout_width="wrap_content" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageSearch"
                        android:layout_height="@dimen/_19pxh"
                        android:layout_width="@dimen/_19pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_search_gray_400"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_search_gray_400"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtHomeFour"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_3pxv"
                        android:layout_marginStart="@dimen/_16pxh"
                        android:layout_marginTop="@dimen/_2pxv"
                        android:layout_weight="0.85"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{drawerItemMenuModel.txtHomeFour}"
                        style="@style/txtInterregular14_1" tools:text="@string/msg_search_structur"/>
                </LinearLayout>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearItems3"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_24pxh"
                    android:layout_marginStart="@dimen/_24pxh"
                    android:layout_marginTop="@dimen/_32pxv"
                    android:layout_width="wrap_content" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageSearchOne"
                        android:layout_height="@dimen/_19pxh"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_width="@dimen/_19pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_search_gray_400"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_search_gray_400"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtHomeFive"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_16pxh"
                        android:layout_marginTop="@dimen/_3pxv"
                        android:layout_weight="0.85"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{drawerItemMenuModel.txtHomeFive}"
                        style="@style/txtInterregular14_1" tools:text="@string/lbl_search_by_vr_id"/>
                </LinearLayout>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearItems4"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_24pxh"
                    android:layout_marginStart="@dimen/_24pxh"
                    android:layout_marginTop="@dimen/_32pxv"
                    android:layout_width="wrap_content" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageCombinedShape"
                        android:layout_height="@dimen/_20pxh"
                        android:layout_width="@dimen/_19pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_combinedshape"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_combinedshape"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtHomeSix"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_16pxh"
                        android:layout_marginTop="@dimen/_4pxv"
                        android:layout_weight="0.74"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{drawerItemMenuModel.txtHomeSix}"
                        style="@style/txtInterregular14_1" tools:text="@string/lbl_sign_out"/>
                </LinearLayout>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearItems5"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_80pxv"
                    android:layout_marginEnd="@dimen/_24pxh"
                    android:layout_marginStart="@dimen/_24pxh"
                    android:layout_marginTop="@dimen/_341pxv"
                    android:layout_width="wrap_content" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageRefresh"
                        android:layout_height="@dimen/_16pxh"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_width="@dimen/_19pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_refresh"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_refresh"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtHomeSeven"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_16pxh"
                        android:layout_marginTop="@dimen/_2pxv"
                        android:layout_weight="0.63"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{drawerItemMenuModel.txtHomeSeven}"
                        style="@style/txtInterregular14_1" tools:text="@string/lbl_sync"/>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
