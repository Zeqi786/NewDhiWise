<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="addWomanOneVM" type="com.syedzulqarnainsapplication.app.modules.addwomanone.data.viewmodel.AddWomanOneVM"/>
    </data>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearAddWomanOne"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylegray_200">
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearAppbartop"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStyleteal_400">
            <androidx.appcompat.widget.Toolbar android:gravity="start"
                android:id="@+id/toolbarToolbar"
                android:layout_height="wrap_content"
                android:layout_marginHorizontal="@dimen/_19pxh"
                android:layout_marginVertical="@dimen/_19pxv"
                android:layout_width="match_parent" app:contentInsetStart="0dp">
                <LinearLayout android:id="@+id/linearRowarrowleft"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageArrowleft"
                        android:layout_height="@dimen/_18pxh"
                        android:layout_width="@dimen/_18pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrowleft"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtVaccinatorTask"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_100pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_weight="0.84"
                        android:layout_width="0dp"
                        android:lineSpacingExtra="@dimen/_1pxh"
                        android:singleLine="true"
                        android:text="@{addWomanOneVM.addWomanOneModel.txtVaccinatorTask}"
                        style="@style/txtIntermedium16" tools:text="@string/lbl_add_woman2"/>
                </LinearLayout>
            </androidx.appcompat.widget.Toolbar>
        </LinearLayout>
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearItems"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylegray_200">
            <LinearLayout android:gravity="start"
                android:id="@+id/linearDatacard"
                android:layout_height="match_parent"
                android:layout_marginBottom="@dimen/_20pxv"
                android:layout_marginEnd="@dimen/_16pxh"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_16pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylecornerRadius">
                <LinearLayout android:id="@+id/linearRowtext"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:orientation="horizontal" style="@style/groupStylegray_100">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtText"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_19pxv"
                        android:layout_marginStart="@dimen/_16pxh"
                        android:layout_marginTop="@dimen/_18pxv"
                        android:layout_weight="0.44"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{addWomanOneVM.addWomanOneModel.txtText}"
                        style="@style/txtIntersemibold14" tools:text="@string/lbl_children"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtTextOne"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_16pxv"
                        android:layout_marginEnd="@dimen/_16pxh"
                        android:layout_marginStart="@dimen/_165pxh"
                        android:layout_marginTop="@dimen/_20pxv"
                        android:layout_weight="0.56"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{addWomanOneVM.addWomanOneModel.txtTextOne}"
                        style="@style/txtIntersemibold14_2" tools:text="@string/lbl_step_2_of_2"/>
                </LinearLayout>
                <FrameLayout android:id="@+id/frameStacktextTwo"
                    android:layout_height="@dimen/_80pxv"
                    android:layout_width="@dimen/_328pxh" style="@style/groupStylewhite_A700cornerRadius">
                    <LinearLayout android:id="@+id/linearField"
                        android:layout_gravity="bottom|center"
                        android:layout_height="wrap_content"
                        android:layout_marginHorizontal="@dimen/_16pxh"
                        android:layout_marginVertical="@dimen/_16pxv"
                        android:layout_width="match_parent"
                        android:orientation="vertical" style="@style/groupStylewhite_8">
                        <TextView android:ellipsize="end"
                            android:gravity="start"
                            android:id="@+id/txtTextTwo"
                            android:layout_height="match_parent"
                            android:layout_marginBottom="@dimen/_13pxv"
                            android:layout_marginEnd="@dimen/_16pxh"
                            android:layout_marginStart="@dimen/_16pxh"
                            android:layout_marginTop="@dimen/_12pxv"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@{addWomanOneVM.addWomanOneModel.txtTextTwo}"
                            style="@style/txtInterregular14_1" tools:text="@string/lbl_1"/>
                    </LinearLayout>
                    <TextView android:ellipsize="end"
                        android:gravity="center"
                        android:id="@+id/txtLabel"
                        android:layout_gravity="top|start"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_16pxv"
                        android:layout_marginEnd="@dimen/_24pxh"
                        android:layout_marginStart="@dimen/_24pxh"
                        android:layout_marginTop="@dimen/_16pxv"
                        android:layout_width="match_parent"
                        android:paddingEnd="@dimen/_8pxh"
                        android:paddingStart="@dimen/_8pxh"
                        android:singleLine="true"
                        android:text="@{addWomanOneVM.addWomanOneModel.txtLabel}"
                        style="@style/txtSolid_1" tools:text="@string/msg_number_of_alive2"/>
                </FrameLayout>
                <View android:background="@color/gray_300"
                    android:id="@+id/lineLineTwo"
                    android:layout_height="@dimen/_1pxv" android:layout_width="@dimen/_328pxh"/>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearRowtextThree"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:orientation="horizontal" style="@style/groupStylewhite_A700">
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearColumntextThree"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_16pxv"
                        android:layout_marginStart="@dimen/_16pxh"
                        android:layout_marginTop="@dimen/_18pxv"
                        android:layout_weight="0.89"
                        android:layout_width="0dp" android:orientation="vertical">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtTextThree"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{addWomanOneVM.addWomanOneModel.txtTextThree}"
                            style="@style/txtIntersemibold14" tools:text="@string/lbl_shahzain_ali"/>
                        <LinearLayout android:gravity="start"
                            android:id="@+id/linearRowgender"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_marginTop="@dimen/_12pxv"
                            android:layout_width="wrap_content" android:orientation="horizontal">
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtGender"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/_2pxv"
                                android:layout_width="wrap_content"
                                android:singleLine="true"
                                android:text="@{addWomanOneVM.addWomanOneModel.txtGender}"
                                style="@style/txtInterregular12" tools:text="@string/lbl_male"/>
                            <View
                                android:background="@drawable/rectangle_bg_gray_400_radius_2"
                                android:id="@+id/viewEllipseOne"
                                android:layout_height="@dimen/_4pxh"
                                android:layout_marginBottom="@dimen/_6pxv"
                                android:layout_marginStart="@dimen/_6pxh"
                                android:layout_marginTop="@dimen/_4pxv" android:layout_width="@dimen/_4pxh"/>
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtDuration"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="@dimen/_6pxh"
                                android:layout_marginTop="@dimen/_1pxv"
                                android:layout_width="wrap_content"
                                android:singleLine="true"
                                android:text="@{addWomanOneVM.addWomanOneModel.txtDuration}"
                                style="@style/txtInterregular12" tools:text="@string/lbl_2_years"/>
                        </LinearLayout>
                    </LinearLayout>
                    <ImageButton
                        android:background="@drawable/rectangle_bg_teal_400_radius_16"
                        android:id="@+id/btnCheckmark"
                        android:layout_height="@dimen/_32pxh"
                        android:layout_marginBottom="@dimen/_22pxv"
                        android:layout_marginEnd="@dimen/_16pxh"
                        android:layout_marginTop="@dimen/_22pxv"
                        android:layout_width="@dimen/_32pxh"
                        android:paddingHorizontal="@dimen/_8pxh"
                        android:paddingVertical="@dimen/_8pxv"
                        android:scaleType="fitCenter"
                        android:src="@drawable/img_checkmark_32x32" tools:ignore="ContentDescription"/>
                </LinearLayout>
                <LinearLayout android:id="@+id/linearRowsubmit"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_24pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <LinearLayout
                        android:id="@+id/linearRowarrowleftOne"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_12pxv"
                        android:layout_marginStart="@dimen/_1pxh"
                        android:layout_marginTop="@dimen/_12pxv"
                        android:layout_weight="0.41"
                        android:layout_width="0dp" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageArrowleftOne"
                            android:layout_height="@dimen/_14pxh"
                            android:layout_marginTop="@dimen/_1pxv"
                            android:layout_width="@dimen/_14pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_arrowleft_14x14"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft_14x14"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtTextbtn"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_marginStart="@dimen/_17pxh"
                            android:layout_weight="0.73"
                            android:layout_width="0dp"
                            android:singleLine="true"
                            android:text="@{addWomanOneVM.addWomanOneModel.txtTextbtn}"
                            android:textAllCaps="true"
                            style="@style/txtIntersemibold14_3" tools:text="@string/lbl_back"/>
                    </LinearLayout>
                    <androidx.appcompat.widget.AppCompatButton
                        android:ellipsize="end" android:gravity="center"
                        android:id="@+id/btnSubmit"
                        android:layout_height="@dimen/_40pxv"
                        android:layout_marginStart="@dimen/_154pxh"
                        android:layout_width="@dimen/_103pxh"
                        android:paddingBottom="@dimen/_13pxv"
                        android:paddingEnd="@dimen/_24pxh"
                        android:paddingStart="@dimen/_24pxh"
                        android:paddingTop="@dimen/_13pxv"
                        android:singleLine="true"
                        android:text="@string/lbl_submit"
                        android:textAllCaps="false"
                        style="@style/btnSolidRounded" tools:text="@string/lbl_submit"/>
                </LinearLayout>
                <View android:background="@color/gray_200"
                    android:id="@+id/viewMarginbottom"
                    android:layout_height="@dimen/_40pxv"
                    android:layout_marginTop="@dimen/_24pxv" android:layout_width="@dimen/_328pxh"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
