<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="scanQrCodeFiveVM" type="com.syedzulqarnainsapplication.app.modules.scanqrcodefive.data.viewmodel.ScanQrCodeFiveVM"/>
    </data>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearScanQRcodeFi"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylegray_200">
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearAppbartop"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStyleteal_400">
            <TextView android:ellipsize="end"
                android:gravity="center_horizontal"
                android:id="@+id/txtVaccinatorTask"
                android:layout_height="match_parent"
                android:layout_marginBottom="@dimen/_17pxv"
                android:layout_marginEnd="@dimen/_125pxh"
                android:layout_marginStart="@dimen/_125pxh"
                android:layout_marginTop="@dimen/_22pxv"
                android:layout_width="match_parent"
                android:lineSpacingExtra="@dimen/_1pxh"
                android:singleLine="true"
                android:text="@{scanQrCodeFiveVM.scanQrCodeFiveModel.txtVaccinatorTask}"
                style="@style/txtIntermedium16" tools:text="@string/lbl_scan_qr_code"/>
        </LinearLayout>
        <FrameLayout android:id="@+id/frameStackrectangleone"
            android:layout_height="@dimen/_720pxv" android:layout_width="match_parent">
            <ImageView android:id="@+id/imageRectangleOne"
                android:layout_gravity="start"
                android:layout_height="match_parent"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                imageRes="@{@drawable/img_rectangle1}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle1"/>
            <FrameLayout android:id="@+id/frameStackrectangletwo"
                android:layout_gravity="start"
                android:layout_height="@dimen/_720pxv" android:layout_width="match_parent">
                <View android:background="@color/bluegray_50"
                    android:id="@+id/viewRectangleTwo"
                    android:layout_gravity="bottom|center"
                    android:layout_height="@dimen/_127pxv"
                    android:layout_marginBottom="@dimen/_273pxv"
                    android:layout_marginEnd="@dimen/_109pxh"
                    android:layout_marginStart="@dimen/_109pxh"
                    android:layout_marginTop="@dimen/_273pxv" android:layout_width="@dimen/_139pxh"/>
                <FrameLayout android:id="@+id/frameStacksubtract"
                    android:layout_gravity="start"
                    android:layout_height="@dimen/_720pxv" android:layout_width="match_parent">
                    <ImageView android:id="@+id/imageSubtract"
                        android:layout_gravity="start"
                        android:layout_height="match_parent"
                        android:layout_width="match_parent"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_subtract"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_subtract"/>
                    <LinearLayout
                        android:id="@+id/linearColumnalignQrCodeWithTheFrameToScan"
                        android:layout_gravity="top|center"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_24pxv"
                        android:layout_marginEnd="@dimen/_40pxh"
                        android:layout_marginStart="@dimen/_40pxh"
                        android:layout_marginTop="@dimen/_24pxv"
                        android:layout_width="match_parent" android:orientation="vertical">
                        <androidx.appcompat.widget.AppCompatButton
                            android:ellipsize="end"
                            android:gravity="center"
                            android:id="@+id/btnAlignQrCodeWithTheFrameToScan"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:paddingBottom="@dimen/_17pxv"
                            android:paddingEnd="@dimen/_16pxh"
                            android:paddingStart="@dimen/_16pxh"
                            android:paddingTop="@dimen/_17pxv"
                            android:singleLine="true"
                            android:text="@string/msg_align_qr_code_w"
                            android:textAllCaps="false"
                            style="@style/btnSolidRounded_2" tools:text="@string/msg_align_qr_code_w"/>
                        <FrameLayout
                            android:id="@+id/frameStack1200pxqrcode"
                            android:layout_gravity="center_horizontal"
                            android:layout_height="@dimen/_181pxv"
                            android:layout_marginEnd="@dimen/_48pxh"
                            android:layout_marginStart="@dimen/_50pxh"
                            android:layout_marginTop="@dimen/_99pxv" android:layout_width="@dimen/_181pxh">
                            <ImageView
                                android:id="@+id/image1200pxQRcode"
                                android:layout_gravity="center"
                                android:layout_height="@dimen/_137pxh"
                                android:layout_marginBottom="@dimen/_19pxv"
                                android:layout_marginEnd="@dimen/_23pxh"
                                android:layout_marginStart="@dimen/_21pxh"
                                android:layout_marginTop="@dimen/_25pxv"
                                android:layout_width="@dimen/_137pxh"
                                android:scaleType="fitXY"
                                imageRes="@{@drawable/img_1200pxqrcode}"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_1200pxqrcode"/>
                            <ImageView android:id="@+id/imageLocation"
                                android:layout_gravity="top|start"
                                android:layout_height="@dimen/_37pxh"
                                android:layout_marginBottom="@dimen/_10pxv"
                                android:layout_marginEnd="@dimen/_10pxh"
                                android:layout_width="@dimen/_37pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_location"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_location"/>
                            <ImageView android:id="@+id/imageMap"
                                android:layout_gravity="top|end"
                                android:layout_height="@dimen/_37pxh"
                                android:layout_marginBottom="@dimen/_10pxv"
                                android:layout_marginStart="@dimen/_10pxh"
                                android:layout_width="@dimen/_37pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_map"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_map"/>
                            <ImageView android:id="@+id/imageClock"
                                android:layout_gravity="bottom|start"
                                android:layout_height="@dimen/_37pxh"
                                android:layout_marginEnd="@dimen/_10pxh"
                                android:layout_marginTop="@dimen/_10pxv"
                                android:layout_width="@dimen/_37pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_clock_37x37"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_clock_37x37"/>
                            <ImageView
                                android:id="@+id/imageRectangleEight"
                                android:layout_gravity="bottom|end"
                                android:layout_height="@dimen/_37pxh"
                                android:layout_marginStart="@dimen/_10pxh"
                                android:layout_marginTop="@dimen/_10pxv"
                                android:layout_width="@dimen/_37pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_rectangle8"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle8"/>
                        </FrameLayout>
                    </LinearLayout>
                </FrameLayout>
            </FrameLayout>
        </FrameLayout>
    </LinearLayout>
</layout>
