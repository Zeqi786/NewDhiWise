                        android:layout_marginTop="@dimen/_41pxv"
                        android:layout_width="@dimen/_15pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_edit_14x15"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_edit_14x15"/>
                </LinearLayout>
                <View android:background="@color/gray_300"
                    android:id="@+id/lineDivider"
                    android:layout_height="@dimen/_1pxv" android:layout_width="@dimen/_328pxh"/>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearRowcurrentaddress"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_8pxv"
                    android:layout_marginEnd="@dimen/_16pxh"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_marginTop="@dimen/_8pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearColumncurrentaddress"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_weight="0.95"
                        android:layout_width="0dp" android:orientation="vertical">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtCurrentAddress"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{familyProfileVM.familyProfileModel.txtCurrentAddress}"
                            style="@style/txtIntermedium12_5" tools:text="@string/lbl_current_address"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtStructure14B"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_marginTop="@dimen/_9pxv"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@{familyProfileVM.familyProfileModel.txtStructure14B}"
                            style="@style/txtInterregular14_1" tools:text="@string/msg_structure_14_b"/>
