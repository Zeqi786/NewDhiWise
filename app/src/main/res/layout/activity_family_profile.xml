                        android:layout_height="@dimen/_40pxh"
                        android:layout_marginBottom="@dimen/_42pxv"
                        android:layout_marginStart="@dimen/_16pxh"
                        android:layout_marginTop="@dimen/_16pxv"
                        android:layout_width="@dimen/_40pxh"
                        android:paddingHorizontal="@dimen/_9pxh"
                        android:paddingVertical="@dimen/_9pxv"
                        android:scaleType="fitCenter"
                        android:src="@drawable/img_user" tools:ignore="ContentDescription"/>
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearColumntextaddressone"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_16pxv"
                        android:layout_marginStart="@dimen/_16pxh"
                        android:layout_marginTop="@dimen/_18pxv"
                        android:layout_weight="0.79"
                        android:layout_width="0dp" android:orientation="vertical">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtTextaddressOne"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{familyProfileVM.familyProfileModel.txtTextaddressOne}"
                            style="@style/txtIntersemibold16" tools:text="@string/lbl_shazia_ali"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtVridCounter"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_marginTop="@dimen/_8pxv"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@{familyProfileVM.familyProfileModel.txtVridCounter}"
                            style="@style/txtIntermedium14_1" tools:text="@string/lbl_vr_id_89389304"/>
                        <LinearLayout android:gravity="start"
                            android:id="@+id/linearRowduration"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_marginTop="@dimen/_7pxv"
                            android:layout_width="match_parent" android:orientation="horizontal">
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtDuration"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/_1pxv"
                                android:layout_width="match_parent"
                                android:singleLine="true"
                                android:text="@{familyProfileVM.familyProfileModel.txtDuration}"
                                style="@style/txtIntermedium14" tools:text="@string/lbl_age_23_years"/>
                            <View
                                android:background="@drawable/rectangle_bg_gray_400_radius_2"
                                android:id="@+id/viewEllipseOne"
                                android:layout_height="@dimen/_4pxh"
                                android:layout_marginBottom="@dimen/_7pxv"
                                android:layout_marginStart="@dimen/_6pxh"
                                android:layout_marginTop="@dimen/_5pxv" android:layout_width="@dimen/_4pxh"/>
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtChildCounter"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/_2pxv"
                                android:layout_marginStart="@dimen/_6pxh"
                                android:layout_width="wrap_content"
                                android:singleLine="true"
                                android:text="@{familyProfileVM.familyProfileModel.txtChildCounter}"
                                style="@style/txtIntermedium14" tools:text="@string/lbl_1_child"/>
