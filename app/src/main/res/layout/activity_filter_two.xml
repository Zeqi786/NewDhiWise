<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="filterTwoVM" type="com.syedzulqarnainsapplication.app.modules.filtertwo.data.viewmodel.FilterTwoVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearFilterTwo"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylegray_200">
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearAppbartop"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStyleteal_400">
            <androidx.appcompat.widget.Toolbar android:gravity="start"
                android:id="@+id/toolbarToolbar"
                android:layout_height="wrap_content"
                android:layout_marginHorizontal="@dimen/_19pxh"
                android:layout_marginVertical="@dimen/_19pxv"
                android:layout_width="match_parent" app:contentInsetStart="0dp">
                <LinearLayout android:id="@+id/linearRowarrowleft"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageArrowleft"
                        android:layout_height="@dimen/_18pxh"
                        android:layout_width="@dimen/_18pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrowleft"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtVaccinatorTask"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_123pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_weight="0.68"
                        android:layout_width="0dp"
                        android:lineSpacingExtra="@dimen/_1pxh"
                        android:singleLine="true"
                        android:text="@{filterTwoVM.filterTwoModel.txtVaccinatorTask}"
                        style="@style/txtIntermedium16" tools:text="@string/lbl_filter"/>
                </LinearLayout>
            </androidx.appcompat.widget.Toolbar>
        </LinearLayout>
        <FrameLayout android:id="@+id/frameStackheading"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_331pxv"
            android:layout_marginBottom="@dimen/_373pxv"
            android:layout_marginEnd="@dimen/_16pxh"
            android:layout_marginStart="@dimen/_16pxh"
            android:layout_marginTop="@dimen/_16pxv" android:layout_width="@dimen/_328pxh">
            <LinearLayout android:id="@+id/linearItems"
                android:layout_gravity="top|start"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_10pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylecornerRadius">
                <LinearLayout android:gravity="center_horizontal"
                    android:id="@+id/linearDatacard"
                    android:layout_height="match_parent"
                    android:layout_width="match_parent"
                    android:orientation="vertical" style="@style/groupStylecornerRadius">
                    <LinearLayout android:gravity="center_horizontal"
                        android:id="@+id/linearColumnheading"
                        android:layout_height="match_parent"
                        android:layout_width="match_parent"
                        android:orientation="vertical" style="@style/groupStylewhite_A700cornerRadius">
                        <EditText
                            android:background="@drawable/rectangle_bg_gray_100"
                            android:ellipsize="end"
                            android:hint="@string/msg_filter_tasks_li"
                            android:id="@+id/etHeading"
                            android:inputType="text"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:paddingBottom="@dimen/_19pxv"
                            android:paddingEnd="@dimen/_30pxh"
                            android:paddingStart="@dimen/_16pxh"
                            android:paddingTop="@dimen/_18pxv"
                            android:singleLine="true"
                            android:text="@={filterTwoVM.filterTwoModel.etHeadingValue}"
                            android:textColorHint="@color/bluegray_900"
                            style="@style/etSolid" tools:ignore="Autofill"/>
                        <Spinner
                            android:background="@drawable/layer_list_bg_white_a700_border_teal_400_radius_4"
                            android:gravity="center_horizontal"
                            android:id="@+id/spinnerField"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_16pxh"
                            android:layout_marginStart="@dimen/_16pxh"
                            android:layout_marginTop="@dimen/_24pxv"
                            android:layout_width="match_parent" tools:listitem="@layout/spinner_item"/>
                        <LinearLayout
                            android:gravity="center_horizontal"
                            android:id="@+id/linearColumn"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_24pxv"
                            android:layout_marginEnd="@dimen/_16pxh"
                            android:layout_marginStart="@dimen/_16pxh"
                            android:layout_marginTop="@dimen/_24pxv"
                            android:layout_width="wrap_content"
                            android:orientation="vertical" style="@style/groupStylewhite_4">
                            <TextView android:ellipsize="end"
                                android:gravity="center_horizontal"
                                android:id="@+id/txtButtontext"
                                android:layout_height="match_parent"
                                android:layout_marginBottom="@dimen/_13pxv"
                                android:layout_marginEnd="@dimen/_24pxh"
                                android:layout_marginStart="@dimen/_24pxh"
                                android:layout_marginTop="@dimen/_12pxv"
                                android:layout_width="match_parent"
                                android:singleLine="true"
                                android:text="@{filterTwoVM.filterTwoModel.txtButtontext}"
                                style="@style/txtIntersemibold14" tools:text="@string/lbl_apply_filter"/>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearDropdown"
                android:layout_gravity="bottom|center"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_16pxh"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStylewhite_6">
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearColumnmenu1menu2m"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_weight="0.94"
                    android:layout_width="0dp"
                    android:orientation="vertical" style="@style/groupStylewhite_A700">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtMenu1Menu2M"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_27pxv"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{filterTwoVM.filterTwoModel.txtMenu1Menu2M}"
                        style="@style/txtInterregular14_1" tools:text="@string/lbl_rehri_goth"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtLanguage"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_24pxv"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{filterTwoVM.filterTwoModel.txtLanguage}"
                        style="@style/txtInterregular14_1" tools:text="@string/lbl_chata_gabool"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtMenu1Menu2MOne"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_23pxv"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{filterTwoVM.filterTwoModel.txtMenu1Menu2MOne}"
                        style="@style/txtInterregular14_1" tools:text="@string/lbl_gharibabad"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtMenu1Menu2MTwo"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_24pxv"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{filterTwoVM.filterTwoModel.txtMenu1Menu2MTwo}"
                        style="@style/txtInterregular14_1" tools:text="@string/lbl_jamali_goth"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtMenu1Menu2MThree"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_20pxv"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_24pxv"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{filterTwoVM.filterTwoModel.txtMenu1Menu2MThree}"
                        style="@style/txtInterregular14_1" tools:text="@string/msg_khuda_bakhsh_go"/>
                </LinearLayout>
                <LinearLayout android:gravity="center_horizontal"
                    android:id="@+id/linearColumnline1stroke"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_weight="0.06"
                    android:layout_width="0dp"
                    android:orientation="vertical" style="@style/groupStylegray_101">
                    <View android:background="@color/gray_401"
                        android:id="@+id/lineLine1Stroke"
                        android:layout_gravity="center_horizontal"
                        android:layout_height="@dimen/_157pxv"
                        android:layout_marginBottom="@dimen/_42pxv"
                        android:layout_marginEnd="@dimen/_6pxh"
                        android:layout_marginStart="@dimen/_6pxh"
                        android:layout_marginTop="@dimen/_16pxv" android:layout_width="@dimen/_4pxh"/>
                </LinearLayout>
            </LinearLayout>
        </FrameLayout>
    </LinearLayout>
</layout>
